// Bean de documentos de admision del alumno
package  adm.alumno.spring;

public class AdmAcomodo{
	private String acomodoId;
	private String nombreAcomodo;	
	private String tipoAcomodo;
	private String generoAcomodo;
	
	public AdmAcomodo(){
		acomodoId 				= "0";
		nombreAcomodo 			= "";
		tipoAcomodo				= "S";
		generoAcomodo 			= "H";
	}

	public String getAcomodoId() {
		return acomodoId;
	}
	public void setAcomodoId(String acomodoId) {
		this.acomodoId = acomodoId;
	}

	public String getNombreAcomodo() {
		return nombreAcomodo;
	}
	public void setNombreAcomodo(String nombreAcomodo) {
		this.nombreAcomodo = nombreAcomodo;
	}

	public String getTipoAcomodo() {
		return tipoAcomodo;
	}
	public void setTipoAcomodo(String tipoAcomodo) {
		this.tipoAcomodo = tipoAcomodo;
	}

	public String getGeneroAcomodo() {
		return generoAcomodo;
	}
	public void setGeneroAcomodo(String generoAcomodo) {
		this.generoAcomodo = generoAcomodo;
	}
	
}