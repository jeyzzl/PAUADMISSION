								                	estadoCivil="Soltero";
								                }else{
								                	estadoCivil="Soltera";
								                }
						             		}else if(solicitud.getEstadoCivil().equals("V")){
								                if(solicitud.getGenero().equals("M")){
								                	estadoCivil="Viudo";
